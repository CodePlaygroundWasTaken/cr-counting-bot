  },
  "devDependencies": {
    "@types/mongodb": "^3.1.31",
    "@types/node": "^14.14.5",
    "@types/ws": "^7.2.9",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
