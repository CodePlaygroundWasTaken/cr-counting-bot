{
  "name": "cr_counting_bot",
  "version": "2.4.2",
  "description": "a bot to manage the counting channel of crs discord server",
  "main": "dist/bot.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
